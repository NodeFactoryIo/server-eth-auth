        "@babel/core": "^7.3.3",
        "@babel/preset-env": "^7.3.1",
        "@babel/preset-typescript": "^7.3.3",
        "@babel/register": "^7.0.0",
        "@types/bn.js": "^4.11.5",
        "@types/chai": "^4.1.7",
        "@types/mocha": "^5.2.5",
        "@types/node": "^12.0.10",
        "@typescript-eslint/eslint-plugin": "^1.3.0",
        "@typescript-eslint/parser": "^1.3.0",
        "chai": "^4.2.0",
        "eslint": "5.16.0",
